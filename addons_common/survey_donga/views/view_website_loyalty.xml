<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="detail_rank_inherit" inherit_id="website_loyalty.website_loyalty_kn">
        <xpath expr="//div[@id='survey-kn']" position="replace">
            <div class="text text-center">
                <a role="button" class="btn btn-primary btn-lg text-center mb-3"
                   t-att-href="'/survey/start-loyalty/%s/%s' % (slug(survey),slug(card_detail.partner_id))">
                    Khảo sát Hà Nội
                </a>
                <a role="button" class="btn btn-primary btn-lg text-center mb-3"
                   t-att-href="'/survey/start-loyalty/%s/%s' % (slug(survey_hcm),slug(card_detail.partner_id))">
                    Khảo sát Hồ Chí Minh
                </a>
            </div>
        </xpath>
    </template>
</odoo>