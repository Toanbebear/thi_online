        <record id="email_template_aplicant_state" model="mail.template">
            <field name="name">Thư mời phỏng vấn</field>
            <field name="model_id" ref="sci_hrms.model_hr_applicant_state"/>
            <field name="partner_to">${object.partner_id.id}</field>
            <field name="subject">Thư mời phỏng vấn</field>
            <field name="body_html">
                <![CDATA[
                    <table class="MsoNormalTable" border="1" cellspacing="0" cellpadding="0" align="left" width="608" style="border: none; margin-left: 6.75pt; margin-right: 6.75pt;">
