<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sh_paperformat_appointmentreceipt" model="report.paperformat">
            <field name="name">Appointment Receipt</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">100</field>
            <field name="page_width">100</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">3</field>
            <field name="margin_bottom">3</field>
            <field name="margin_left">3</field>
            <field name="margin_right">3</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">3</field>
            <field name="dpi">130</field>
        </record>
        <record id="sh_paperformat_test_result" model="report.paperformat">
            <field name="name">Phiếu kết quả- Bệnh viện</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <!--            <field name="page_height"></field>-->
            <!--            <field name="page_width"></field>-->
            <field name="orientation">Portrait</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">15</field>
            <field name="margin_right">8</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>
        <record id="sh_paperformat_prescription" model="report.paperformat">
            <field name="name">Đơn thuốc - Bệnh viện</field>
            <field name="default" eval="True"/>
            <field name="format">A5</field>
            <!--            <field name="page_height"></field>-->
            <!--            <field name="page_width"></field>-->
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">8</field>
            <field name="margin_right">4</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">2</field>
            <field name="dpi">130</field>
        </record>

        <record id="sh_paperformat_medical_payment" model="report.paperformat">
            <field name="name">Phiếu thu - Bệnh viện</field>
            <field name="default" eval="True"/>
            <field name="format">A5</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">3</field>
            <field name="dpi">130</field>
        </record>

        <!--        SonZ-->
        <record id="sh_paperformat_stock_picking_bill" model="report.paperformat">
            <field name="name">Phiếu kho</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">8</field>
            <field name="margin_right">8</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">2</field>
            <field name="dpi">130</field>
        </record>
        <!--        SonZ-->
    </data>
</odoo>
